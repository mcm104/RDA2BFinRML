#has identifier for work (RDA 6.8)
#test

<#identifiedByMapping>
  a rr:TriplesMap ;

  rr:logicalSource [
    rml:source "" ;
    rml:referenceFormulation ""
  ] ;

  rr:subjectMap [
    rml:reference "" ;
    rr:class bf:Work
  ] ;

  rr:predicateObjectMap [
    rr:predicate bf:identifiedBy ;
    rr:objectMap [
      rr:parentTriplesMap <#IdentifierMapping>
    ]
  ] .

<#IdentifierMapping>
  a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "" ;
    rml:referenceFormulation ""
  ] ;

  rr:subjectMap [
    rr:termType rr:BlankNode ;
    rr:class bf:Identifier
  ] ;

  rr:predicateObjectMap [
    rr:predicate rdf:value ;
    rr:objectMap [
      rr:reference ""
    ]
  ] .

